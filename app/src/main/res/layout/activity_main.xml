<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <RelativeLayout
        android:id="@+id/rl18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true">

        <TextView
            android:id="@+id/tv18"
            android:layout_width="23dp"
            android:layout_height="wrap_content"
            android:text="18"
            android:textAlignment="center" />

        <RelativeLayout
            android:id="@+id/btrl18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/tv18">

            <ImageView
                android:id="@+id/trapesium_atas_18"
                android:layout_width="23dp"
                android:layout_height="8dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/ic_gigi_trapesium_atas" />

            <ImageView
                android:id="@+id/trapesium_kiri_18"
                android:layout_width="8dp"
                android:layout_height="23dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/ic_gigi_trapesium_kiri" />

            <ImageView
                android:id="@+id/trapesium_kanan_18"
                android:layout_width="9dp"
                android:layout_height="23dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="-1dp"
                android:background="@drawable/ic_gigi_trapesium_kanan" />

            <ImageView
                android:id="@+id/trapesium_bawah_18"
                android:layout_width="24dp"
                android:layout_height="8dp"
                android:layout_alignStart="@+id/trapesium_atas_18"
                android:layout_alignLeft="@+id/trapesium_atas_18"
                android:layout_alignTop="@+id/trapesium_atas_18"
                android:layout_marginStart="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/ic_gigi_trapesium_bawah" />

            <ImageView
                android:id="@+id/kotak_18"
                android:layout_width="7dp"
                android:layout_height="8dp"
                android:layout_above="@+id/trapesium_bawah_18"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginBottom="-16dp"
                android:cropToPadding="false"
                android:src="@drawable/ic_gigi_kotak_tengah"/>
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/rl18">

        <TextView
            android:id="@+id/tv17"
            android:layout_width="23dp"
            android:layout_height="wrap_content"
            android:text="17"
            android:textAlignment="center"/>

        <RelativeLayout
            android:id="@+id/btrl17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_below="@+id/tv17">

            <ImageView
                android:id="@+id/trapesium_atas_17"
                android:layout_width="23dp"
                android:layout_height="8dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/ic_gigi_trapesium_atas" />

            <ImageView
                android:id="@+id/trapesium_kiri_17"
                android:layout_width="8dp"
                android:layout_height="23dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="0dp"
                android:background="@drawable/ic_gigi_trapesium_kiri" />

            <ImageView
                android:id="@+id/trapesium_kanan_17"
                android:layout_width="9dp"
                android:layout_height="23dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="15dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="-1dp"
                android:background="@drawable/ic_gigi_trapesium_kanan" />

            <ImageView
                android:id="@+id/trapesium_bawah_17"
                android:layout_width="24dp"
                android:layout_height="8dp"
                android:layout_alignStart="@+id/trapesium_atas_17"
                android:layout_alignLeft="@+id/trapesium_atas_17"
                android:layout_alignTop="@+id/trapesium_atas_17"
                android:layout_marginStart="0dp"
                android:layout_marginLeft="0dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/ic_gigi_trapesium_bawah" />

            <ImageView
                android:id="@+id/kotak_17"
                android:layout_width="7dp"
                android:layout_height="8dp"
                android:layout_above="@+id/trapesium_bawah_17"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginBottom="-16dp"
                android:cropToPadding="false"
                android:background="@drawable/ic_gigi_kotak_tengah" />
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>